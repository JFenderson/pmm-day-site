{
  "name": "pmm-day-site",
  "version": "1.0.0",
  "description": "web site for the 2019 Purple Marching Machine Family and Friend Picnic",
  "main": "server/lib/index.js",
  "engines": {
    "node": "8.9.4",
    "npm": "6.1.0"
  },
  "scripts": {
    "build:product": "webpack --mode production",
    "build:client": "webpack",
    "build:server": "babel ./server/src -d ./server/lib",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rimraf ./client/dist",
    "clean:server": "rimraf ./server/lib",
    "dev": "npm run clean && npm run build:server && concurrently -r -k \"npm run watch:client\" \"npm run watch:server\" \"nodemon --watch server/lib --delay 1\"",
    "start": "npm run clean && npm run build:client && npm run build:server && npm run start:server",
    "start:server": "node server/lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "babel ./server/src -w -d ./server/lib"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "animate.css": "^3.6.1",
    "aws-sdk": "^2.277.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "card": "^2.4.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "firebase": "^5.2.0",
    "firebase-admin": "^5.12.1",
    "humanparser": "^1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "jquery-modal": "^0.9.1",
    "libphonenumber-js": "^1.3.1",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.18.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.4",
    "nodemailer-mailgun-transport": "^1.4.0",
    "nodemailer-sendinblue-transport": "^1.2.3",
    "request": "^2.87.0",
    "rimraf": "^2.6.2",
    "sib-api-v3-sdk": "^7.0.1",
    "slick-carousel": "^1.8.1",
    "stripe": "^6.0.0",
    "uuid": "^3.3.2",
    "webpack": "^4.8.2",
    "webpack-command": "^0.4.1",
    "zipcodes": "^6.0.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.17.4",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
