{
  "name": "pmm-day-site",
  "version": "1.0.0",
  "description": "web site for the 2019 Purple Marching Machine Family and Friend Picnic",
  "main": "server/lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "webpack",
    "build:server": "babel ./server/src -d ./server/lib",
    "dev": "source .env && concurrently -k \"npm run watch:client\" \"npm run watch:server\"",
    "prepare": "npm run build:server",
    "start": "npm run build:client && npm run build:server && npm run start:server",
    "start:server": "node server/lib/index.js",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon \"server/lib\" --watch \"server/src\"",
    "sass:build": "node-sass -r client/index.scss client/dist/index.css",
    "sass:watch": "node-sass --w client/index.scss -o client/dist/index.css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^3.6.1",
    "aws-sdk": "^2.238.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "body-parser": "^1.18.2",
    "card": "^2.4.0",
    "cors": "^2.8.4",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "jquery-modal": "^0.9.1",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.18.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.4",
    "stripe": "^6.0.0",
    "webpack": "^4.8.2"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
